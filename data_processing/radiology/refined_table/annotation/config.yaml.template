# mind root data path
ROOT_PATH: /path/to/data/path

# dicom table path - remove once all tables follow the same convention.
DICOM_TABLE_PATH: /path/to/dicom/table

PROJECT: DMT_IRB

DATASET_NAME: DMT_IRB_DATE

DATA_TYPE: PNG

# SQL to filter dicom proxy table.
